@model komp.Models.naudotojas
@{
    ViewBag.Title = "Prisijungti";
}

<h2>Prisijungti</h2>
<body>
    <form action='@Url.Action("Login", "User")' method="post">
        <div class="container">
           
            <div class="editor-field">
                <label>El. paštas</label>
                @Html.EditorFor(model => model.elpastas)
            </div>

            <div class="editor-field">
                <label>Slaptažodis</label>
                @Html.EditorFor(model => model.slaptazodis)
            </div>


            <input type="submit" value="Submit" />

        </div>
    </form>
</body>